{
  "dxt_version": "0.2",
  "name": "claude-mcp",
  "version": "1.1.2",
  "description": "The official MCP Server for the Claude API",
  "author": {
    "name": "Claude"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kappasig920/mcp-server-deep-research.git"
  },
  "homepage": "https://github.com/Kappasig920/mcp-server-deep-research/tree/main/packages/mcp-server#readme",
  "server": {
    "type": "node",
    "entry_point": "index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "PETSTORE_API_KEY": "${user_config.PETSTORE_API_KEY}"
      }
    }
  },
  "user_config": {
    "PETSTORE_API_KEY": {
      "title": "api_key",
      "description": "",
      "required": true,
      "type": "string"
    }
  },
  "tools": [],
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
